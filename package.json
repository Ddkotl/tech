{
  "name": "tech",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"npm run start_crone_parse\"",
    "build": "next build",
    "start": "concurrently \"next start\" \"npm run start_crone_parse\"",
    "lint": "next lint",
    "lint:types": "tsc --noEmit",
    "prettier": "prettier -w .",
    "test": "jest",
    "test:e2e": "playwright test",
    "seed": "tsx prisma/seed.ts",
    "parse": "tsx src/shared/lib/parsing/index.ts",
    "start_crone_parse": "wait-on http://localhost:3000/auth/sign-in && curl -X GET http://localhost:3000/api/crone/parse",
    "prepare": "husky install && playwright install",
    "restore:minio": "tsx src/shared/lib/backups/s3/s3_restore.ts",
    "restore:db": "tsx src/shared/lib/backups/db/db_restore.ts",
    "restore": "bun run restore:minio && npm run restore:db",
    "backup": "tsx src/shared/lib/backups/db/db_backup.ts && tsx src/shared/lib/backups/s3/s3_backup.ts"
  },
  "dependencies": {
